[ Template
    [ Text "Infobox country" ]
    [ ( Just "largest_city" , [ Text "Kabul" ] )
    , ( Just "languages"
      , [ Template
            [ Text "hlist" ]
            [ ( Just "style" , [ Text "white-space:nowrap; " ] )
            , ( Nothing
              , [ InternalLink
                    LinkTarget
                      { linkTargetPage = PageName { getPageName = "Pashto" }
                      , linkTargetAnchor = Nothing
                      }
                    []
                ]
              )
            , ( Nothing
              , [ InternalLink
                    LinkTarget
                      { linkTargetPage = PageName { getPageName = "Dari language" }
                      , linkTargetAnchor = Nothing
                      }
                    [ [ Text "Dari" ] ]
                ]
              )
            ]
        , XmlOpenClose "ref" [ ( "name" , "AO" ) ]
        ]
      )
    , ( Just "demonym"
      , [ InternalLink
            LinkTarget
              { linkTargetPage =
                  PageName { getPageName = "Demography of Afghanistan" }
              , linkTargetAnchor = Nothing
              }
            [ [ Text "Afghan" ] ]
        , MagicWord
            "tag"
            [ ( Nothing , [ Text "ref" ] )
            , ( Nothing
              , [ Text
                    "Other terms that have been used as demonyms are '''Afghani'''"
                , XmlTag
                    "ref"
                    []
                    [ Text "Dictionary.com. "
                    , InternalLink
                        LinkTarget
                          { linkTargetPage =
                              PageName
                                { getPageName =
                                    "The American Heritage Dictionary of the English Language"
                                }
                          , linkTargetAnchor = Nothing
                          }
                        []
                    , Text ", Fourth Edition. Houghton Mifflin Company, 2004. "
                    , ExternalLink
                        (Url "http://dictionary.reference.com/browse/afghani")
                        (Just "Reference.com")
                    , Text " (Retrieved 13 November 2007)."
                    ]
                , Text " and '''Afghanistani'''."
                , XmlTag
                    "ref"
                    []
                    [ Text "Dictionary.com. "
                    , InternalLink
                        LinkTarget
                          { linkTargetPage = PageName { getPageName = "WordNet" }
                          , linkTargetAnchor = Nothing
                          }
                        []
                    , Text " 3.0. "
                    , InternalLink
                        LinkTarget
                          { linkTargetPage =
                              PageName { getPageName = "Princeton University" }
                          , linkTargetAnchor = Nothing
                          }
                        []
                    , Text ". "
                    , ExternalLink
                        (Url "http://dictionary.reference.com/browse/afghanistani")
                        (Just "Reference.com")
                    , Text " (Retrieved 13 November 2007). "
                    , Template [ Text "webarchive" ] []
                    ]
                ]
              )
            , ( Just "name" , [ Text "\"Demonym\"" ] )
            , ( Just "group" , [ Text "\"Note\"" ] )
            ]
        ]
      )
    , ( Just "government_type"
      , [ InternalLink
            LinkTarget
              { linkTargetPage = PageName { getPageName = "Unitary state" }
              , linkTargetAnchor = Nothing
              }
            [ [ Text "Unitary" ] ]
        , Text " "
        , InternalLink
            LinkTarget
              { linkTargetPage = PageName { getPageName = "Presidential system" }
              , linkTargetAnchor = Nothing
              }
            [ [ Text "presidential" ] ]
        , Text " "
        , InternalLink
            LinkTarget
              { linkTargetPage = PageName { getPageName = "Islamic republic" }
              , linkTargetAnchor = Nothing
              }
            []
        ]
      )
    ]
]