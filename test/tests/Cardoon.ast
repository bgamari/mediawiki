[ Heading 2 [ Text "Description" ]
, Text "The wild cardoon is a stout "
, InternalLink
    LinkTarget
      { linkTargetPage = PageName { getPageName = "herbaceous plant" }
      , linkTargetAnchor = Nothing
      }
    [ [ Text "herbaceous" ] ]
, Text " "
, InternalLink
    LinkTarget
      { linkTargetPage = PageName { getPageName = "perennial plant" }
      , linkTargetAnchor = Nothing
      }
    []
, Text " growing "
, Template
    "convert"
    [ ( Nothing , [ Text "0.8" ] )
    , ( Nothing , [ Text "to" ] )
    , ( Nothing , [ Text "1.5" ] )
    , ( Nothing , [ Text "m" ] )
    , ( Nothing , [ Text "in" ] )
    , ( Just "abbr" , [ Text "on" ] )
    ]
, Text
    " tall, with deeply lobed and heavily spined green to grey-green "
, InternalLink
    LinkTarget
      { linkTargetPage = PageName { getPageName = "tomentose" }
      , linkTargetAnchor = Nothing
      }
    []
, Text " (hairy or downy) leaves up to "
, Template
    "convert"
    [ ( Nothing , [ Text "50" ] )
    , ( Nothing , [ Text "cm" ] )
    , ( Nothing , [ Text "0" ] )
    , ( Just "abbr" , [ Text "on" ] )
    ]
, Text
    " long, with yellow spines up to 3.5&nbsp;cm long. The flowers are violet-purple, produced in a large, "
, InternalLink
    LinkTarget
      { linkTargetPage = PageName { getPageName = "globose" }
      , linkTargetAnchor = Nothing
      }
    []
, Text ", massively spined "
, InternalLink
    LinkTarget
      { linkTargetPage = PageName { getPageName = "head (botany)" }
      , linkTargetAnchor = Nothing
      }
    [ [ Text "capitulum" ] ]
, Text " up to "
, Template
    "convert"
    [ ( Nothing , [ Text "6" ] )
    , ( Nothing , [ Text "cm" ] )
    , ( Nothing , [ Text "0" ] )
    , ( Just "abbr" , [ Text "on" ] )
    ]
, Text " in diameter."
, XmlTag
    "ref"
    [ ( "name" , "Sonnante" ) ]
    [ Text
        "Sonnante, G., Pignone, D, & Hammer, K. (2007). The Domestication of Artichoke and Cardoon: From Roman Times to the Genomic Age. "
